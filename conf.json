{
	"plugins": [],
	"recurseDepth": 10,
	"source": {
		"include": ["lib", "package.json", "README.md"],
		"includePattern": ".+\\.js(doc|x)?$",
		"excludePattern": "(^|\\/|\\\\)_"
	},
	"sourceType": "module",
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc","closure"],
		"useLongnameInNav": false,
		"showInheritedInNav": true
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false
	},
	"opts": {
		"destination": "./out/",
		"encoding": "utf8",
		"private": true,
		"recurse": true,
		"template": "./minami"
	}
}