{
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc"]
	},
	"source": {
		"include": ["lib", "package.json", "README.md"],
		"includePattern": ".js$",
		"excludePattern": "(node_modules/|out)"
	},
	"plugins": [
		"plugins/markdown"
	],
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": true,
		"useLongnameInNav": false,
		"showInheritedInNav": true
	},
	"opts": {
		"encoding": "utf8",
		"private": true,
		"recurse": true
	}
}